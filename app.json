{
    "name": "KennedyX Music",
    "logo": "https://telegra.ph/file/73cbefa8401982900fe81.jpg",
    "description": "KennedyX Music allow you to stream music trought the telegram voice chat feature.",
    "keywords": ["music", "voice chat", "telegram"],
    "repository": "https://github.com/KennedyProject/KennedyXMusic",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "isi dengan string Pyrogram (P)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "bot tokenmu dari @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "nama bot musik mu.",
            "required": false,
            "value": ""
        },
        "BOT_USERNAME": {
            "description": "isi dengan username bot musikmu tanpa menggunakan @",
            "required": true
        },
        "BOT_IMG": {
            "description": "Photo untuk alive.",
            "required": false,
            "value": "https://telegra.ph/file/5f5f8e412644c47cc836a.jpg"
        },
        "ASSISTANT_NAME": {
            "description": "isi dengan username asistenmu tanpa menggunakan @",
            "required": true
        },
        "BG_IMAGE": {
            "description": "isi dengan link telegraph background mu",
            "required": false,
            "value": "https://telegra.ph/file/20147c4f049e2c1f2f248.png"
        },
        "OWNER_NAME": {
            "description": "isi dengan username mu tanpa menggunakan @",
            "required": false,
            "value": "abhinasroy"
        },
        "API_ID": {
            "description": "APP ID mu dari my.telegram.org",
            "required": true
        },
        "API_HASH": {
            "description": "API HASH dari my.telegram.org",
            "required": true
        },
        "SUDO_USERS": {
            "description": "id untuk daftar anggota sudo (pisahkan dengan spasi).",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "max durasi untuk video yang diunduh (dalam menit).",
            "required": true,
            "value": "60"
        },
        "PMPERMIT": {
            "description": "pm-permit untuk asisten.",
            "required": false,
            "value": "ENABLE"
        },
        "UPDATES_CHANNEL": {
            "description": "isi dengan username channel kamu, jika tidak punya lewati saja",
            "required": false,
            "value": "ABOUT_ABHINAS"
        },
        "GROUP_SUPPORT": {
            "description": "isi dengan username grup kamu, jika tidak punya skip saja",
            "required": false,
            "value": "DOSTI_GROUP_1234"
        }
    },
    "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
    }
  ]
}
